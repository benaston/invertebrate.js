<script src="/src/vendor/invertebrate.js/app.js"></script>
<script src="/src/vendor/invertebrate.js/config.js"></script>
<script src="/src/vendor/invertebrate.js/templateServerSvc.js"></script>
<script src="/src/vendor/invertebrate.js/model.js"></script>
<script src="/src/vendor/invertebrate.js/view.js"></script>
<script src="/src/vendor/invertebrate.js/router.js"></script>
